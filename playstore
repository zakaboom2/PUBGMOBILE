on = '[🅾🅽]'
off = '[🅾🅵🅵]'
cstatus_1 = off
HOME = 1
function HOME()
MENU = gg.choice({
"━═✿ AUTO CLICK ✿═━\n• ᴡᴀʟʟʜᴀᴄᴋ & ᴡᴀʀɴᴀ",
"━═✿ NO RICOIL ✿═━\n• ᴋᴀᴅᴀɴɢ ᴇʀʀᴏʀ ʙɪʟᴀ ʙᴇʀʟᴇʙɪʜ",
"━═✿ LESS RICOIL ✿═━\n• ʀᴇᴄᴏᴍᴇɴᴅ",
cstatus_1 .." SIT SCOPE\n• ᴋᴇᴛɪᴋᴀ sᴛᴀᴛᴜs ᴏɴ sᴀᴀᴛ ᴅɪ ʟᴏʙʙʏ ᴅɪ ᴍᴀᴛɪᴋᴀɴ",
"━═✿ FUNCTION ✿ ═━\n• ᴍᴀɴᴜᴀʟ sᴇᴛ ʟᴀɴᴅɪɴɢ & ʟᴏʙʙʏ",
"━═✿ NOROOT / ROOT ✿═━\n• ᴠɪʀᴛᴜᴀʟ & ᴀɴᴛɪ ʀᴇᴘᴏʀᴛ( ᴠᴀʟᴜᴇ ᴛᴇsᴛ )",
"•EXIT•"}, nil, "                                     🅱🅰🅹🅸🅶🆄🆁 🆃🅴🅰🅼\n                                         PUBG MOBILE 0.9.1\n                                             °• PLAYSTORE •°")
if MENU == 1 then auto() end
if MENU == 2 then NCoil2() end
if MENU == 3 then NCoil() end
if MENU == 5 then fun() end
if MENU == 6 then del() end
if MENU == 7 then menu2()
elseif MENU == nil then
elseif MENU == 4 then
   if cstatus_1 == off then SO(on)
   cstatus_1 = on
	else
	  SF(off)
	  cstatus_1 = off 
	end
   end
  jhv=-1
end

function hiden()
gg.alert("LOCK")
end


-- WALLHACK COLOR --
function WHM()
MWCL = gg.multiChoice({
	"━═✿ ᴡᴀʟʟʜᴀᴄᴋ • ⁴⁰⁰ ╾",
	"━═✿ ᴡᴀʟʟʜᴀᴄᴋ • ⁴¹⁰ ╾",
	"━═✿ ᴡᴀʟʟʜᴀᴄᴋ • ⁴²⁵ ╾",
	"━═✿ ᴡᴀʟʟʜᴀᴄᴋ • ⁴³⁵ ╾",	
	"━═✿ ᴡᴀʟʟʜᴀᴄᴋ • ⁶²⁵ ╾",
	"━═✿ ᴡᴀʟʟʜᴀᴄᴋ • ⁶⁶⁰ ╾",
	"━═✿ ᴡᴀʟʟʜᴀᴄᴋ • ⁸³⁵ ╾",
	"━═✿ ᴡᴀʟʟʜᴀᴄᴋ • ⁸⁴⁵ ╾",
	"━═✿ ғɪx sᴄᴏᴘᴇ ʜɪʟᴀɴɢ ╾",
	"╼  ᴡᴀʀɴᴀ • ʜɪᴊᴀᴜ ╾",
	"╼  ᴡᴀʀɴᴀ • pιnĸ ╾",
	"╼  ᴡᴀʀɴᴀ • ᴍᴇʀᴀʜ ╾",
	"╼  ᴡᴀʀɴᴀ • ᴋᴜɴɪɴɢ ╾",
	"╼  ᴡᴀʀɴᴀ • ʜɪᴊᴀᴜ ⁸³⁵ ⁻ ⁴³⁵ ╾",
	"╼  ᴡᴀʀɴᴀ • ᴍᴇʀᴀʜ ⁸³⁵ ⁻ ⁴³⁵ ╾",
	"╼  ᴡᴀʀɴᴀ • ᴋᴜɴɪɴɢ ⁸³⁵ ⁻ ⁴³⁵ ╾",
	"ᴋ ᴇ ᴍ ʙ ᴀ ʟ ɪ ⤵"
},nil,"°•MENU WALLHACK°•")
if MWCL == nil then 
else 
if MWCL[1] == true then Wh400() end
if MWCL[2] == true then Wh410() end
if MWCL[3] == true then Wh425() end
if MWCL[4] == true then Wh435() end
if MWCL[5] == true then Wh625() end
if MWCL[6] == true then Wh660() end
if MWCL[7] == true then Wh835() end
if MWCL[8] == true then Wh845() end
if MWCL[9] == true then FixScope() end
if MWCL[10] == true then CLH() end
if MWCL[11] == true then CLPink() end
if MWCL[12] == true then CLM() end
if MWCL[13] == true then CLK() end
if MWCL[14] == true then CLH8() end
if MWCL[15] == true then CLM8() end
if MWCL[16] == true then CLK8() end
if MWCL[17] == true then HOME() end
end
jhv= -1
end


--- WALLHACK
function Wh400()
gg.clearResults()
gg.setRanges(gg.REGION_BAD)
gg.searchNumber("4,141D;4.7408155e21;-5.5693206e-40;4.814603e21;3.7615819e-37;2:", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.searchNumber("2", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(4)
gg.editAll("120", gg.TYPE_FLOAT)
gg.clearResults()
gg.searchNumber("-1.0285578e-38;3.7615819e-37;2;-1;1;-127::300", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.searchNumber("2", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(4)
gg.editAll("120", gg.TYPE_FLOAT)
gg.clearResults()
gg.searchNumber("304.00009155273;3.7615819e-37;2;-1;1;-127::240", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.searchNumber("2", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(4)
gg.editAll("120", gg.TYPE_FLOAT)
gg.toast(" 😄 ᴄʜᴇᴀᴛᴇʀ ɴᴜᴜʙ 😄 ")
end

