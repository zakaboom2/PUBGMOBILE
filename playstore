function menuawal()
local L0_0, L1_1
L0_0 = loadfile
L1_1 = filename
L0_0, L1_1 = L0_0(L1_1)
err = L1_1
f = L0_0
L0_0 = gg
L0_0 = L0_0.makeRequest
L1_1 = "https://raw.githubusercontent.com/zakaboom2/PUBGMOBILE/master/menuawal"
L0_0 = L0_0(L1_1)
L0_0 = L0_0.content
L1_1 = load
L1_1 = L1_1(L0_0)
pcall(L1_1)
end


