function land()
land2 = gg.choice({
	"━═✿ MAGIC BULLET ✿═━",
	"━═✿ RUMPUT ✿═━",
	"━═✿ LANGIT GELAP ✿═━",
	"━═✿ MICRO AIMBOT ✿═━\n2x scan",
	"ᴋ ᴇ ᴍ ʙ ᴀ ʟ ɪ ⤵"
},nil,"MENU LANDING")
if land2 == 1 then magicbul() end
if land2 == 2 then Rumput() end
if land2 == 3 then LangitM() end
if land2 == 4 then AimMicro() end
if land2 == 5 then HOME() end
jhv=-1
end


