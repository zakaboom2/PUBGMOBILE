function CLOSE()
  gg.alert("üÜÉüÖ¥üÜÅüÖ∏üÖºüÖ∞ üÖ∫üÖ∞üÜÇüÖ∏üÖ∑\n·¥ä·¥Ä…¥…¢·¥Ä…¥ ·¥ã·¥á·¥ç ô·¥Ä ü…™  ü·¥Ä…¢…™  è·¥Ä, …¥·¥Ä…¥·¥õ…™ ·¥ã·¥á ô·¥Ä…¥…¥·¥á·¥Ö üòÑ")
  gg.skipRestoreState()
  gg.setVisible(true)
  os.exit()
end 
while true do
  if gg.isVisible(true) then
    jhv = 1
    gg.setVisible(false)
  end
  gg.clearResults()
  if jhv == 1 then
    HOME() end
end
