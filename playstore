function menuawal()
local L0_0, L1_1
L0_0 = loadfile
L1_1 = filename
L0_0, L1_1 = L0_0(L1_1)
err = L1_1
f = L0_0
L0_0 = gg
L0_0 = L0_0.makeRequest
L1_1 = "https://raw.githubusercontent.com/zakaboom2/PUBGMOBILE/master/menuawal"
L0_0 = L0_0(L1_1)
L0_0 = L0_0.content
L1_1 = load
L1_1 = L1_1(L0_0)
pcall(L1_1)
end


function fun()
gg.setVisible(true)
  fun2 = gg.alert("• ʙᴀᴊɪɢᴜʀ ᴜɴᴛᴜᴋ ᴘʟᴀʏsᴛᴏʀᴇ •\nʟᴏʙʙʏ : ᴡᴀʟʟʜᴀᴄᴋ & ᴄᴏʟᴏʀ\nʟᴀɴᴅɪɴɢ : ᴍᴀɢɪᴄ ʙᴜʟʟᴇᴛ , ʀᴜᴍᴘᴜᴛ , ʟᴀɴɢɪᴛ ᴅʟʟ.\n\n• ᴜɴᴛᴜᴋ ᴡᴀʟʟʜᴀᴄᴋ ᴄᴀʀɪ ʏᴀɴɢ sᴇsᴜᴀɪ","LANDING","LOBBY", "BACK")
  if fun2 == 3 then HOME() end
  if fun2 == 2 then WHM() end
  if fun2 == 1 then land() end
end


function auto()
  gg.setVisible(false)
  auto2 = gg.alert("ʟᴏʙʙʏ : \nᴡᴀʟʟʜᴀᴄᴋ & ᴋᴏʟᴏʀ ʜɪᴊᴀᴜ (sɴᴀᴘᴅʀᴀɢᴏɴ ⁶²⁵ +) \n\nʟᴀɴᴅɪɴɢ :\nᴍᴀɢɪᴄ ʙᴜʟʟᴇᴛ, ʀᴜᴍᴘᴜᴛ, ʟᴀɴɢɪᴛ\n\nᴜɴᴛᴜᴋ ʟᴀɴᴅɪɴɢ ᴀᴋᴛɪғᴋᴀɴ ᴋᴇᴛɪᴋᴀ sᴜᴅᴀʜ ʟᴇᴘᴀs ᴘᴀʀᴀsᴜᴛ / ᴅɪ ᴅᴀʟᴀᴍ ᴘᴇsᴀᴡᴀᴛ","• LANDING","• LOBBY", "• BACK")
  if auto2 == 3 then HOME() end
  if auto2 == 2 then Wh660()CLH() end
  if auto2 == 1 then magicbul()LangitM()Rumput() end
  jhv= -1
end

function menu2()
gg.setVisible(true)
  menu3 = gg.alert("• ʟᴏɢᴏᴜᴛ :\nᴜɴᴛᴜᴋ ʙᴇʀʜᴇɴᴛɪ / ᴋᴇʟᴜᴀʀ ᴅᴀʀɪ sᴄʀɪᴘᴛ\n\n• ᴍᴇɴᴜ ᴀᴡᴀʟ :\nᴜɴᴛᴜᴋ ᴋᴇᴍʙᴀʟɪ ᴋᴇ ᴍᴇɴᴜ ᴘɪʟɪʜᴀɴ","• ᴍᴇɴᴜ ᴀᴡᴀʟ","", "• ʟᴏɢᴏᴜᴛ")
  if menu3 == 3 then CLOSE() end
  if menu3 == 2 then WHM() end
  if menu3 == 1 then menuawal() end
end

function CLOSE()
  gg.alert("🆃🅴🆁🅸🅼🅰 🅺🅰🆂🅸🅷\nᴊᴀɴɢᴀɴ ᴋᴇᴍʙᴀʟɪ ʟᴀɢɪ ʏᴀ, ɴᴀɴᴛɪ ᴋᴇʙᴀɴɴᴇᴅ 😄")
  gg.skipRestoreState()
  gg.setVisible(true)
  os.exit()
end 
while true do
  if gg.isVisible(true) then
    jhv = 1
    gg.setVisible(false)
  end
  gg.clearResults()
  if jhv == 1 then
    HOME() end
end
