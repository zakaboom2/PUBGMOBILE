HOME = 1
function HOME()
MENU = gg.choice({
"• Playstore ",
"• Crack",
"• Lebih Sadis",
"•EXIT•"
}, nil, "                                     🅱🅰🅹🅸🅶🆄🆁 🆃🅴🅰🅼\n                                         PUBG MOBILE 0.9.0\n                                              °• ʝнσиνσ∂σ •°")
if MENU == 1 then Play() end
if MENU == 2 then Crack() end
if MENU == 3 then Lebih() end
if MENU == 4 then CLOSE() end
end

